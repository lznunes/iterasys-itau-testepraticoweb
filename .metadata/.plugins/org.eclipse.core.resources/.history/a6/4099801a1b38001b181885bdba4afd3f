	//Gera pasta para salvar logs
	public static String gerapastalog() {
		Date date = new Date();
		SimpleDateFormat DateFor = new SimpleDateFormat("yyyy-MM-dd hhmmss");
		String stringdate = DateFor.format(date);
		return stringdate;
	}


	
	
	

